import java.util.Scanner;

public class proba {

    public static void main(String[] args) {
        int a, b, c;
        Scanner sc = new Scanner(System.in);
        System.out.println("Введите три переменные");
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        System.out.println(a + b + c);
        double d = Math.pow(b, 2) - 4 * a * c;
        System.out.println(d);
        double x, xx;
        if(d == 0){
            x = (- 1) * b * Math.pow((2 * a), -1);
            System.out.println("уравнение имеет один корень: " + x);
        }
        if(d > 0){
            x = ((- 1) * b + Math.sqrt(d)) * Math.pow((2 * a), -1);
            xx = ((- 1) * b - Math.sqrt(d)) * Math.pow((2 * a), -1);
            System.out.println("уравнение имеет два корня: " + x + " " + xx);
        }
        if(d < 0){
            System.out.println("уравнение не имеет корней");
        }
    }
}
